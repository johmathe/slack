<VirtualHost *>
	ServerAdmin webmaster@nonutc.fr
	ServerName shotdetect.nonutc.fr

        RedirectMatch ^/$ /shotdetect/

	DocumentRoot /home/www/shotdetect/

        <Directory />
                Options FollowSymLinks
                AllowOverride None
        </Directory>
        <Directory /home/www/shotdetect >
                Options Indexes FollowSymLinks MultiViews
                AllowOverride None
                Order allow,deny
                allow from all
        </Directory>

<Location /svn>
  DAV svn
  SVNPath /home/svn/shotdetect
  SVNAutoversioning on
  AuthzSVNAccessFile /home/johmathe/svn_access_file
  AuthType Basic
  AuthName "SVN - shotedetct"
  AuthUserFile /home/johmathe/trac.htpasswd
  Require valid-user
</Location>

Alias /trac "/usr/share/trac/htdocs"
ScriptAlias /shotdetect /usr/share/trac/cgi-bin/trac.cgi

<Location "/shotdetect">
 SetEnv TRAC_ENV "/home/trac/shotdetect"
</Location>
  
<Directory "/usr/share/trac/htdocs">
  Options Indexes MultiViews
  AllowOverride None
  Order allow,deny
  Allow from all
</Directory>
  
<Location "/shotdetect/login">
  AuthType Basic
  AuthName "Shotdetect"
  AuthUserFile /home/johmathe/trac.htpasswd
  Require valid-user
</Location>


	ErrorLog /var/log/apache2/error.log

	# Possible values include: debug, info, notice, warn, error, crit,
	# alert, emerg.
	LogLevel warn

</VirtualHost>

# See man 5 aliases for format

#listes de diffusion

admins:              "|/var/lib/mailman/mail/mailman post admins"
admins-admin:        "|/var/lib/mailman/mail/mailman admin admins"
admins-bounces:      "|/var/lib/mailman/mail/mailman bounces admins"
admins-confirm:      "|/var/lib/mailman/mail/mailman confirm admins"
admins-join:         "|/var/lib/mailman/mail/mailman join admins"
